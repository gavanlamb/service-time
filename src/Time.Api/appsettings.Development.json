{
  "Logging": {
    "SerilogConfiguration": {
      "Using":  [
        "Serilog.Sinks.Console"
      ],
      "MinimumLevel": {
        "Default": "Debug",
        "Override": {
          "Microsoft": "Debug",
          "System": "Debug"
        }
      },
      "WriteTo": [
        {
          "Name": "Console",
          "Args": {
            "formatter": "Serilog.Formatting.Compact.RenderedCompactJsonFormatter, Serilog.Formatting.Compact"
          }
        }
      ],
      "Enrich": [
        "FromLogContext",
        "WithMachineName",
        "WithThreadId",
        "WithAssemblyVersion"
      ],
      "Properties": {
        "Application": "Time"
      }
    }
  }
}