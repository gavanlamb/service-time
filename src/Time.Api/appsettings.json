{
  "Logging": {
    "SerilogConfiguration": {
      "Using": [
        "AWS.Logger.SeriLog"
      ],
      "MinimumLevel": {
        "Default": "Information",
        "Override": {
          "Microsoft": "Information",
          "System": "Information"
        }
      },
      "LogGroup": "/time/preview5",
      "Region": "ap-southeast-2",
      "WriteTo": [
        {
          "Name": "AWSSeriLog"
        }
      ],
      "Enrich": [
        "FromLogContext",
        "WithMachineName",
        "WithThreadId",
        "WithAssemblyVersion"
      ],
      "Properties": {
        "Application": "Time"
      }
    }
  },
  "AllowedHosts": "*"
}
